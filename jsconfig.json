      "module": "ESNext",
      "moduleResolution": "Node",
      "target": "ES2020",
      "jsx": "react",
      "lib": [
        "esnext",
        "dom"
      ]
    },
    "include": [
      "./src/**/*",
      "./gatsby-node.js",
